# Required for build-time data fetching
NEXT_PUBLIC_ASSISTANT_ADDRESS=0x...

# Required for server operations
SECRET_KEY=your-secret-key
RPC_URL=https://base-mainnet.g.alchemy.com/v2/your-api-key
SUBGRAPH_URL=your-subgraph-url
ALCHEMY_BEARER=your-alchemy-bearer-token

# Database configuration
DATABASE_URL=your-database-url

# KV Store configuration
KV_REST_API_READ_ONLY_TOKEN=your-kv-read-only-token
KV_REST_API_TOKEN=your-kv-token
KV_REST_API_URL=your-kv-api-url
KV_URL=your-kv-url

# Network configuration
NEXT_PUBLIC_CHAIN_ID=8453

# Optional: Fallback addresses (for backward compatibility)
# These will be used if build-time data fetching fails
NEXT_PUBLIC_ORACLE_ADDRESS=0x...
NEXT_PUBLIC_SIR_ADDRESS=0x...
NEXT_PUBLIC_VAULT_ADDRESS=0x...
# Note: Fee parameters are now fetched at build time from contracts

# Skip environment validation (useful for Docker builds)
# SKIP_ENV_VALIDATION=true
