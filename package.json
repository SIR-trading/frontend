{
  "name": "sir-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run generate:build-data && pnpm run fetch:tokens && next build",
    "generate:build-data": "tsx scripts/generate-build-data.ts",
    "fetch:tokens": "tsx scripts/fetch-token-lists.ts",
    "dev": "pnpm run generate:build-data && pnpm run fetch:tokens && concurrently -n ws,next -c blue,green \"pnpm ws:dev\" \"next dev --turbo\"",
    "lint": "next lint",
    "start": "next start",
    "test": "vitest",
    "type-check": "tsc --noEmit --incremental",
    "build-graph": "graphclient build",
    "prepare": "husky",
    "ws:dev": "pnpm --filter sir-websocket-server dev",
    "ws:build": "pnpm --filter sir-websocket-server build",
    "ws:start": "pnpm --filter sir-websocket-server start"
  },
